<div class="side-bar">
<%= render partial: 'sidebar' %>
</div>
<div class="main-content">
<% @titles.each do |title| %>
<div class="game-info">
 <h4>Title:<%= title.title%></h4>
 <h4>Maker:<%= title.maker%></h4>
 <h4>Series:<%= title.series%></h4>
 <h4>Hard:<%= title.hard%></h4>
 <%= link_to "Show-Play-History", game_path(title.id), class: 'show' %>
 <% if user_signed_in? && current_user.id == 1 %>
 <%= link_to "Delete-Data", game_path(title.id), method: :delete,data: { confirm: '削除しますか？' }, class: 'delete' %>
 <% end %>
</div>
<% end %>
</div>